{
  "name": "quiz-master-server",
  "version": "1.0.0",
  "description": "Real-time quiz platform server with WebSocket support",
  "main": "server/app.js",
  "scripts": {
    "start": "node server/app.js",
    "dev": "nodemon server/app.js",
    "test": "npm run test:server && npm run test:client",
    "test:server": "mocha server/tests/**/*.test.js",
    "test:client": "npm run serve:test",
    "serve": "node server/app.js",
    "serve:dev": "nodemon server/app.js --ignore client/",
    "serve:test": "http-server tests/ -p 8081",
    "install:all": "npm install && npm install -g nodemon",
    "setup": "npm run install:all && mkdir -p logs uploads/temp",
    "lint": "eslint server/ --ext .js",
    "lint:fix": "eslint server/ --ext .js --fix"
  },
  "keywords": [
    "quiz",
    "real-time",
    "websocket",
    "education",
    "interactive",
    "german",
    "live-quiz",
    "scoreboard"
  ],
  "author": "QuizMaster Team",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "rate-limiter-flexible": "^2.4.1",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "chai": "^4.3.7",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1",
    "http-server": "^14.1.1",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/quiz-master.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/quiz-master/issues"
  },
  "homepage": "https://github.com/your-username/quiz-master#readme"
}
